<%= simple_form_for @user, url: wizard_path, method: :put do |f| %>
  <%=  f.input :email  %>
  <%=  f.input :password  %>
  <%=  f.input :password_confirmation  %>

  <%= f.submit "Save and Continue", class: "btn btn-primary" %>
<% end %>


<hr/>
<%= link_to 'skip', next_wizard_path %>

<%= tutorial_progress_bar %>
